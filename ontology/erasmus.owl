        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
        <rdfs:comment>Definition: A prerequisite or entry requirement for accessing the Learning Opportunity Instance.
Comments:</rdfs:comment>
        <rdfs:label>Prerequisite</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#qualification -->

    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#qualification">
