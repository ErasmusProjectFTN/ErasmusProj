        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
        <rdfs:comment>Definition: Number of places available for participants in the Learning Opportunity Instance
Comments:</rdfs:comment>
        <rdfs:label>Places</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#prerequisite -->

