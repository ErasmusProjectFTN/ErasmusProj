    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#assessment -->

    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#assessment">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunityInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
        <rdfs:comment>Definition: assessment strategy: a description of the broad approach to assessment used in the learning
opportunity.
Comments: Examples include types of assessments and evaluations of learning and competency
development such as exams, mostly by coursework etc.</rdfs:comment>
        <rdfs:label>Assessment</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#cost -->

    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#cost">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunityInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Definition: A cost associated with obtaining access to the Learning Opportunity Instance.
Comments:</rdfs:comment>
        <rdfs:label>Cost</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#credit -->

    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#credit">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunitySpecification"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
        <rdfs:comment>Definition: An account of the credits that can be obtained from completion of a Learning Opportunity.</rdfs:comment>
        <rdfs:label>Credit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#duration -->

    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#duration">
