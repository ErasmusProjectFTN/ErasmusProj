    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunityObject -->

    <owl:Class rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunityObject">
        <rdfs:comment>Definition: An abstract resource used within the context of learning opportunities.
Comments:</rdfs:comment>
        <rdfs:label>Learning Opportunity Object</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunityProvider -->

    <owl:Class rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunityProvider">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunityObject"/>
        <rdfs:comment>Definition: An agent (person or organisation) that provides learning opportunities.
Comments:</rdfs:comment>
        <rdfs:label>Learning Opportunity Provider</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunitySpecification -->

    <owl:Class rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunitySpecification">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/Ontology1482491841.owl#LearningOpportunityObject"/>
        <rdfs:comment>Definition: An abstract description of a learning opportunity, consisting of information that will be
consistent across multiple instances of the learning opportunity.
Comments:</rdfs:comment>
        <rdfs:label>Learning Opportunity Specification</rdfs:label>
    </owl:Class>
    


