        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
        <rdfs:comment>Definition: A qualification that can be obtained from completion of a Learning Opportunity.
Comments:</rdfs:comment>
        <rdfs:label>Qualification</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/Ontology1482491841.owl#start -->

    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/Ontology1482491841.owl#start">
